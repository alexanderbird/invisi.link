{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "UpdateHostedZone",
            "Effect": "Allow",
            "Action": [
                "route53:GetHostedZone",
                "route53:ChangeResourceRecordSets",
                "route53:ListResourceRecordSets"
            ],
            "Resource": "arn:aws:route53:::hostedzone/Z0394676AAXOOA2NV0I3"
        },
        {
            "Sid": "GlobalPermissions",
            "Effect": "Allow",
            "Action": [
                "route53:GetChange",
                "route53:ListHostedZones",
                "s3:CreateBucket"
            ],
            "Resource": "*"
        }
    ]
}
